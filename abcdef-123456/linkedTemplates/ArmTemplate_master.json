{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "abcdef-123456"
		},
		"123abcdef123_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of '123abcdef123_sa'"
		},
		"azure_sql_db_111_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azure_sql_db_111'"
		},
		"azure_sql_db_222_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azure_sql_db_222'"
		},
		"azure_sql_db_333_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azure_sql_db_333'"
		},
		"azuresqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuresqldb'"
		},
		"sqlonprem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqlonprem'"
		},
		"sharedir_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/a5404a65-4631-4bb7-ae98-6a05acfeec52/resourcegroups/demo_477/providers/Microsoft.DataFactory/factories/pqrst/integrationruntimes/onprem-ir"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "abcdef-123456_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"123abcdef123_sa_connectionString": {
						"value": "[parameters('123abcdef123_sa_connectionString')]"
					},
					"azure_sql_db_111_connectionString": {
						"value": "[parameters('azure_sql_db_111_connectionString')]"
					},
					"azure_sql_db_222_connectionString": {
						"value": "[parameters('azure_sql_db_222_connectionString')]"
					},
					"azure_sql_db_333_connectionString": {
						"value": "[parameters('azure_sql_db_333_connectionString')]"
					},
					"azuresqldb_connectionString": {
						"value": "[parameters('azuresqldb_connectionString')]"
					},
					"sqlonprem_connectionString": {
						"value": "[parameters('sqlonprem_connectionString')]"
					},
					"sharedir_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('sharedir_properties_typeProperties_linkedInfo_resourceId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "abcdef-123456_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'abcdef-123456_ArmTemplate_0')]"
			]
		}
	]
}