{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFolderPath": "Table2",
							"wildcardFileName": "*.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "cid",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "CID",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "cname",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "CNAME",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "age",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "AGE",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "loc",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "LOC",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "gender",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "GENDER",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "startdate",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "STARTDATE",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Folder",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "sqldatads",
						"type": "DatasetReference",
						"parameters": {
							"servername": "azure-training-sqlserver2.database.windows.net",
							"username": "training",
							"schemaname": "dbo",
							"databasename": "demo",
							"secretname": "sqlserver2",
							"tablename": "stg_customer"
						}
					}
				]
			}
		],
		"annotations": []
	}
}